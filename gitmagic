#checkout file from another branch
	git checkout my-awesome-source-branch the/path/to/yourfile.rad

#diff between a file on two different branches
	git diff mybranch master -- myfile.cs

#git logs with diffs for each commit
	git log -p

#To see a list of which commits are on branch a but not on b
	git log a ^b --no-merges

#To delete a tag locally and then remotely
	git tag -d my-tag
	git push origin :refs/tags/my-tag
